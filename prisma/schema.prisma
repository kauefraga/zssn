generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Survivor {
  id   Int    @id @default(autoincrement())
  name String @unique
  age  Int
  // lastLoc     Location @relation(fields: [locationLat], references: [lat])
